<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox id="menu_pane" alignment="CENTER" opacity="0.64" prefHeight="638.0" prefWidth="631.0" spacing="20" style="-fx-background-color: black;" stylesheets="@../css/menu.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main_menu.PauseController">
      
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>

    <Button id="startButton" fx:id="startButton" onAction="#handleResume" prefHeight="46.0" prefWidth="230.0" style="-fx-font-size: 15px; -fx-text-fill: white; -fx-background-color: black; -fx-border-color: white; -fx-cursor: hand;" text="Continue" />
    <Button onAction="#handleBackToMenu" prefHeight="46.0" prefWidth="230.0" style="-fx-font-size: 15px; -fx-text-fill: white; -fx-background-color: black; -fx-border-color: white; -fx-cursor: hand;" text="Quit" /> 
    
</VBox>
